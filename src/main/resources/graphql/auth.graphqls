### Type ###
type TokenResponse {
    accessToken: String!
    refreshToken: String!
}

input SignInRequest {
    type: String!
    token: String!
}

input SignUpRequest {
    type: String!
    token: String!
    nickname: String!
    email: String!
    phone: String!
    address: String!
    profileImage: Upload
}

input ReissueRequest {
    accessToken: String!
    refreshToken: String!
}