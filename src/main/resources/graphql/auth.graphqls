### Input ###
input SignInRequest {
    type: AuthType!
    token: String!
}

input SignUpRequest {
    type: AuthType!
    token: String!
    name: String!
    email: String!
    phone: String!
    address: String!
    profileImage: Upload
}

input ReissueRequest {
    accessToken: String!
    refreshToken: String!
}

### Type ###
type Token {
    accessToken: String!
    refreshToken: String!
}

enum AuthType {
    KAKAO
    NAVER
}